<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<!-- Relationship -->
		<record id="view_relationship_form" model="ir.ui.view">
			<field name="name">res.relationship.form</field>
			<field name="model">res.relationship</field>
			<field name="arch" type="xml">
				<form string="Relationship">
					<sheet>
						<group>
							<group>
								<field name="name" />
							</group>
							<group>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="view_relationship_tree" model="ir.ui.view">
			<field name="name">res.relationship.tree</field>
			<field name="model">res.relationship</field>
			<field name="arch" type="xml">
				<tree string="Relationship">
					<field name="name" />
				</tree>
			</field>
		</record>

		<record id="action_relationship" model="ir.actions.act_window">
			<field name="name">Relationship</field>
			<field name="res_model">res.relationship</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

